{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"comment_line": {
		"prefix": "comment_line",
		"body": [
			"% -------------------------------------------------------------",
		],
		"description": "comment line"
	},
	"tmp": {
		"prefix": "tmp",
		"body": [
			"\\documentclass{jarticle}",
			"",
			"\\usepackage{listings,jlisting}",
			"\\usepackage{here}",
			"\\usepackage{algorithm}",
			"\\usepackage{algorithmic}",
			"\\usepackage[dvipdfmx]{graphicx}",
			"\\usepackage{wrapfig}",
			"\\usepackage{amsmath}",
			"\\usepackage{comment}",
			"\\usepackage{bm}",
			"\\usepackage{url}",
			"\\usepackage{cleveref_ja}",
			"",
			"% 余白の設定",
			"\\usepackage[top=30truemm,bottom=30truemm,left=25truemm,right=25truemm]{geometry}",
			"",
			"\\lstset{",
			"language = Python,   %プログラム言語(複数の言語に対応，C,C++も可)",
			"breaklines = true,   %枠外に行った時の自動改行",
			"breakindent = 10pt,  %自動改行後のインデント量(デフォルトでは20[pt])  ",
			"basicstyle = \\ttfamily\\scriptsize,  %標準の書体",
			"%枠 \"t\"は上に線を記載, \"T\"は上に二重線を記載",
			"%他オプション：leftline，topline，bottomline，lines，single，shadowbox",
			"frame = tbrl,",
			"framesep = 5pt,      %frameまでの間隔(行番号とプログラムの間)",
			"numbers = left,      %行番号の位置 none,left,right",
			"stepnumber = 1,      %行番号の間隔",
			"numberstyle = \\tiny, %行番号の書体",
			"tabsize = 4,",
			"captionpos = t, % t,b",
			"showstringspaces=false",
			"}",
			"",
			"%ここでタイトルを作ると同じページから本文が開始される",
			"%\\maketitleは\\begin{document}の中に書く",
			"\\title{$1}",
			"\\author{$2}",
			"\\date{$3}",
			"",
			"\\begin{document}",
			"%ここでタイトルを作ると本文開始時に改ページ",
			"%\\title{}",
			"%\\author{}",
			"%\\date{}",
			"\\maketitle",
			"%\\thispagestyle{empty}   %ここからの3行で",
			"%\\clearpage              %表紙にページ番号を表示させず",
			"%\\addtocounter{page}{-1} %次のページからページ番号を開始する",
			"",
			"%\\newpage",
			"",
			"\\setcounter{section}{0}",
			"\\section{$4}",
			"$0",
			"",
			"",
			"",
			"",
			"",
			"\\begin{thebibliography}{数字}",
			"\\bibitem{sanko} ",
			"\\end{thebibliography}",
			"",
			"",
			"\\end{document}"
		],
		"description": "template"
	},
	"tmp2": {
		"prefix": "tmp2",
		"body": [
			"\\documentclass[twocolumn]{jsarticle}",
			"",
			"\\usepackage{listings,jlisting}",
			"\\usepackage{here}",
			"\\usepackage{algorithm}",
			"\\usepackage{algorithmic}",
			"\\usepackage[dvipdfmx]{graphicx}",
			"\\usepackage{wrapfig}",
			"\\usepackage{amsmath}",
			"\\usepackage{comment}",
			"\\usepackage{bm}",
			"\\usepackage{url}",
			"\\usepackage{cleveref_ja}",
			"",
			"% 余白の設定",
			"\\usepackage[top=15truemm,bottom=15truemm,left=15truemm,right=15truemm]{geometry}",
			"\\setlength{\\columnsep}{10truemm}     %左右コラムの間隔の変更。",
			"%\\setlength{\\columnseprule}{0.4pt} %区切り線の太さの変更。",
			"%\\setlength{\\columnwidth}{10cm}    %コラム幅の設定。",
			"",
			"\\lstset{",
			"   language = Python,   %プログラム言語(複数の言語に対応，C,C++も可)",
			"   breaklines = true,   %枠外に行った時の自動改行",
			"   breakindent = 10pt,  %自動改行後のインデント量(デフォルトでは20[pt])",
			"   basicstyle = \\ttfamily\\scriptsize,  %標準の書体",
			"   %枠 \"t\"は上に線を記載, \"T\"は上に二重線を記載",
			"   %他オプション：leftline，topline，bottomline，lines，single，shadowbox",
			"   frame = tbrl,",
			"   framesep = 5pt,      %frameまでの間隔(行番号とプログラムの間)",
			"   numbers = left,      %行番号の位置 none,left,right",
			"   stepnumber = 1,      %行番号の間隔",
			"   numberstyle = \\tiny, %行番号の書体",
			"   tabsize = 4,",
			"   captionpos = t % t,b",
			"}",
			"",
			"",
			"\\title{${1:title}}",
			"\\author{${2:author}}",
			"\\date{${3:\\today}}",
			"",
			"\\begin{document}",
			"",
			"% maketitleの前にabstractを書く",
			"",
			"%必要ならばabstractを書く",
			"%\\begin{abstract}",
			"",
			"%\\end{abstract}",
			"",
			"\\maketitle",
			"",
			"\\setcounter{section}{0}",
			"\\section{$4}",
			"$0",
			"",
			"",
			"",
			"\\begin{thebibliography}{数字}",
			"\\bibitem{sanko} ",
			"\\end{thebibliography}",
			"",
			"",
			"\\end{document}"
		],
		"description": "2 column template"
	},
	"table": {
		"prefix": "table",
		"body": [
			"\\begin{table}[H]",
			"    \\begin{center}",
			"        \\begin{tabular}{|c|c|c|} \\hline",
			"        &  &   \\\\\\ \\hline",
			"        &  &   \\\\\\ \\hline",
			"        &  &   \\\\\\ \\hline",
			"        \\end{tabular}  ",
			"    \\end{center}",
			"\\end{table}"
		],
		"description": "create table"
	},
	"images": {
		"prefix": "images",
		"body": [
			"\\begin{figure}[H]",
			"    \\begin{minipage}{0.33\\hsize}",
			"        \\begin{center}",
			"        \\includegraphics[width=0.99\\columnwidth]{$1}",
			"        \\end{center}",
			"        \\caption{one}",
			"        \\label{fig:one}",
			"    \\end{minipage}",
			"    \\begin{minipage}{0.33\\hsize}",
			"        \\begin{center}",
			"        \\includegraphics[width=0.99\\columnwidth]{$2}",
			"        \\end{center}",
			"        \\caption{two}",
			"        \\label{fig:two}",
			"    \\end{minipage}",
			"    \\begin{minipage}{0.33\\hsize}",
			"        \\begin{center}",
			"        \\includegraphics[width=0.99\\columnwidth]{$3}",
			"        \\end{center}",
			"        \\caption{third}",
			"        \\label{fig:three}",
			"    \\end{minipage}",
			"\\end{figure}",
		],
		"description": "create images"
	},
	"image": {
		"prefix": "image",
		"body": [
			"\\begin{figure}[H]",
			"    \\begin{center}",
			"        \\includegraphics[width=0.8\\columnwidth]{$1}",
			"    \\end{center}",
			"    \\caption{caption}",
			"    \\label{label}",
			"\\end{figure}",
		],
		"description": "include image"
	},
