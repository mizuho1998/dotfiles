{
    "editor.renderWhitespace": "all",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.wordWrap": "on",
    "window.restoreWindows": "none",
    "workbench.startupEditor": "newUntitledFile",
    "breadcrumbs.enabled": true,
    "editor.acceptSuggestionOnEnter": "off",
    "keyboard.dispatch": "keyCode",
    "[html]": {
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.tabSize": 4
    }
}
